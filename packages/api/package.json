  "scripts": {
    "build": "cross-env NODE_ENV=deploy nest build",
    "dev": "pnpm run --stream --parallel /^dev:api:.*$/",
    "dev:api:src": "cross-env NODE_ENV=development nest start --watch",
    "dev:api:static": "http-server ./public -a 127.0.0.1 -p 3003 -g -b -s --cors",
    "start": "cross-env NODE_ENV=deploy node dist/main",
    "devtools": "cross-env NODE_ENV=development ts-node --project ./tsconfig.json -r tsconfig-paths/register ./src/cli.ts",
    "test": "jest --config jest.config.json -u --forceExit --detectOpenHandles --runInBand --passWithNoTests",
    "test:cov": "pnpm run test --coverage"
  },
