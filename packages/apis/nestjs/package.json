{
  "name": "@xiaoshop/api-nestjs",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.6.0",
  "description": "XiaoShop 云链小店 (@api-nestjs)",
  "author": "Luoyi <luoyi@mouji.net>",
  "license": "Apache-2.0",
  "scripts": {
    "build": "cross-env NODE_ENV=deploy nest build",
    "dev": "pnpm run --stream --parallel /^dev:.*$/",
    "dev:static": "http-server ./public -p 3003 -g -b --cors",
    "dev:watch": "cross-env NODE_ENV=development nest start --watch",
    "start": "cross-env NODE_ENV=deploy node dist/main",
    "debug": "pnpm run dev --debug",
    "devtools": "cross-env NODE_ENV=development ts-node --project ./tsconfig.json -r tsconfig-paths/register ./src/cli.ts",
    "test": "jest --config ./tests/jest-e2e.json --watch --detectOpenHandles --updateSnapshot --no-cache",
    "test:cov": "pnpm run test --coverage"
  },
